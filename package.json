{
  "name": "koa-server",
  "version": "1.0.0",
  "description": "A server of koa",
  "repository": {
    "type": "git",
    "url": "https://github.com/lianruhe/koa-server.git"
  },
  "scripts": {
    "start": "webpack --progress --colors --config webpack.config.server.js && node dist/server.bundle.js",
    "server": "concurrently -r -k \"cross-env NODE_ENV=development DEBUG=app* nodemon ./cli/compile.js\" \"node dist/server.bundle.js\""
  },
  "author": "shanks <lianruhe2013@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "koa": "^2.2.0",
    "koa-static": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.1",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "nodemon": "^1.11.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.6.1"
  }
}
